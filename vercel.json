{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.php",
        "use": "vercel-php@0.10.0"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/index.html",
        "dest": "/public/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  